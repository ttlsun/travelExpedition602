<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<!-- SqlMapConfig.xml : 별칭,언더바 설정 -->
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>	
	
	<typeAliases>
		<typeAlias alias="myAddrBean" type="com.my.travelExpedition.model.AddrBean"/>
		<typeAlias alias="myAdminEventBean" type="admin.event.model.AdminEventBean"/>
		<typeAlias alias="myAdminNoticeBean" type="admin.notice.model.AdminNoticeBean"/>
		<typeAlias alias="myAdminQnaBean" type="admin.qna.model.AdminQnaBean"/>
		<typeAlias alias="myCampingBean" type="user.camping.model.CampingBean"/>
		<typeAlias alias="myCommunityBean" type="user.community.model.CommunityBean"/>
		<typeAlias alias="myCommunityReplyBean" type="user.community.model.CommunityBean"/>
		<typeAlias alias="myLikeBean" type="user.like.model.LikeBean"/>
		<typeAlias alias="myPayBean" type="user.pay.model.PayBean"/>
		<typeAlias alias="myPostimgBean" type="user.postimg.model.PostimgBean"/>
		<typeAlias alias="myQnaBean" type="user.qna.model.QnaBean"/>
		<typeAlias alias="myRatingBean" type="user.rating.model.RatingBean"/>
		<typeAlias alias="myReservationBean" type="user.reservation.model.ReservationBean"/>
		<typeAlias alias="myRoomBean" type="user.room.model.RoomBean"/>
		<typeAlias alias="myTourBean" type="user.tour.model.TourBean"/>
		<typeAlias alias="myUsersBean" type="user.users.model.UsersBean"/>
		<typeAlias alias="myKeywordBean" type="user.common.model.KeywordBean"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/Addr.xml"/>
		<mapper resource="mybatis/AdminEvent.xml"/>
		<mapper resource="mybatis/AdminNotice.xml"/>
		<mapper resource="mybatis/AdminQna.xml"/>
		<mapper resource="mybatis/Camping.xml"/>
		<mapper resource="mybatis/Community.xml"/>
		<mapper resource="mybatis/Like.xml"/>
		<mapper resource="mybatis/Pay.xml"/>
		<mapper resource="mybatis/Postimg.xml"/>
		<mapper resource="mybatis/Qna.xml"/>
		<mapper resource="mybatis/Rating.xml"/>
		<mapper resource="mybatis/Reservation.xml"/>
		<mapper resource="mybatis/Room.xml"/>
		<mapper resource="mybatis/Tour.xml"/>
		<mapper resource="mybatis/Users.xml"/>
		<mapper resource="mybatis/Keyword.xml"/>
	</mappers>
</configuration>

